´´´ function FIJO(x0, es, imax, gx)
xr = x0;
iter = 0;
g = str2func(['@(x)' gx]);
do = 0;
fprintf(' iteracion Resultado error\n')
while (do == 0)
xrold = xr;
xr = g(xrold);
iter = iter + 1;
if (xr ~= 0)
ea = abs((xr - xrold) / xr) * 100;
end
fprintf(' %4.0f %4.4f %4.4f\n', iter, xr, ea)
if ((ea < es) || (iter >= imax))
break;
end
end
end ´´´
